{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "0.12.2",

  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/christiankaindl/trello-super-powers",
  "icons": {
    "16": "assets/logos/logo_small.svg",
  	"32": "assets/logos/logo_small.svg",
    "48": "assets/logos/logo_small.svg",
  	"64": "assets/logos/logo.svg",
  	"128": "assets/logos/logo.svg"
  },
  "permissions": [
  	"tabs", "storage", "https://trello.com/b/*/", "notifications", "downloads"
  ],
  "background": {
  	"scripts": ["background.js"]
  },
  "page_action": {
    "browser_style": true,
    "default_icon": {
      "16": "/assets/page-action/page-action-16.svg",
      "19": "/assets/page-action/page-action-19.svg",
      "32": "/assets/page-action/page-action-32.svg",
      "38": "/assets/page-action/page-action-38.svg"
    },
  "default_title": "Export Trello board as CSV",
  "default_popup": "/page-action/index.html"
  },

  "options_ui": {
    "page": "settings/options.html",
    "browser_style": true
  },

  "applications": {
    "gecko": {
      "id": "{e8a71c3b-3deb-4ab3-834a-5c0aee943847}",
      "strict_min_version": "52.0"
    }
  },
  "default_locale": "en"
}
